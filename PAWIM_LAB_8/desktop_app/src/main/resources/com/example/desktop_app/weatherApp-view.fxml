<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="609.0" prefWidth="659.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.desktop_app.viewModel.WeatherAppViewModel">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox alignment="CENTER" prefHeight="119.0" prefWidth="583.0">
      <children>
         <Label text="Aplikacja Weather" textFill="#1022c7fc">
            <font>
               <Font size="36.0" />
            </font>
         </Label>
         <Button mnemonicParsing="false" onAction="#onComputerButtonClick" prefHeight="39.0" prefWidth="149.0" text="Computers App">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
      </children>
   </VBox>
   <TextField fx:id="fxCityNameField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="36.0" prefWidth="230.0" promptText="Nazwa Miasta">
      <font>
         <Font size="18.0" />
      </font>
   </TextField>
   <HBox prefHeight="324.0" prefWidth="583.0" spacing="20.0">
      <children>
         <VBox alignment="CENTER" prefHeight="324.0" prefWidth="254.0" spacing="10.0">
            <children>
               <Button mnemonicParsing="false" onAction="#onTemperatureButtonClick" prefHeight="38.0" prefWidth="93.0" text="Temperatue" />
               <Button mnemonicParsing="false" onAction="#onLastWeatherAlarmButtonClick" prefHeight="38.0" prefWidth="101.0" text="Weather Alarm" />
               <Button mnemonicParsing="false" onAction="#onTemperaturesFrom24HButtonClick" prefHeight="38.0" prefWidth="142.0" text="Temperature From 24H" />
               <Button mnemonicParsing="false" onAction="#onDailyForecastButtonClick" prefHeight="38.0" prefWidth="97.0" text="Daily Forecast" />
               <Button mnemonicParsing="false" onAction="#onMaxTemperatureForNext5DaysButtonClick" prefHeight="38.0" prefWidth="173.0" text="Max Temperature For 5 Days" />
               <Button mnemonicParsing="false" onAction="#onTemperatureFromNext12HoursButtonClick" prefHeight="38.0" prefWidth="164.0" text="Temperature For Next 12H" />
            </children>
            <opaqueInsets>
               <Insets bottom="20.0" top="20.0" />
            </opaqueInsets>
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </VBox>
         <TextArea fx:id="fxText" prefHeight="324.0" prefWidth="330.0">
            <font>
               <Font size="14.0" />
            </font></TextArea>
      </children>
   </HBox>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
