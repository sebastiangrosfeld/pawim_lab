<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" minHeight="-Infinity" minWidth="-Infinity" prefHeight="609.0" prefWidth="915.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.desktop_app.viewModel.CreateEditViewModel">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox alignment="CENTER" prefHeight="94.0" prefWidth="619.0">
      <children>
         <Label text="Aplikacja Computer" textFill="#1022c7fc">
            <font>
               <Font size="36.0" />
            </font>
         </Label>
         <Button mnemonicParsing="false" onAction="#onComputerAppClick" prefHeight="39.0" prefWidth="149.0" text="Computer Table">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
      </children>
   </VBox>
   <HBox alignment="CENTER_LEFT" prefHeight="36.0" prefWidth="619.0" spacing="110.0">
      <padding>
         <Insets left="40.0" right="70.0" />
      </padding>
      <children>
         <TextField fx:id="fxName" promptText="NAME" />
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="490.0" text="Computer Values">
            <font>
               <Font size="18.0" />
            </font>
         </Label>
      </children>
   </HBox>
   <HBox prefHeight="419.0" prefWidth="822.0" spacing="20.0">
      <children>
         <VBox alignment="CENTER" prefHeight="324.0" prefWidth="254.0" spacing="10.0">
            <children>
               <Button mnemonicParsing="false" onAction="#onCreateComputerButtonClick" prefHeight="38.0" prefWidth="134.0" text="Create" />
               <Button mnemonicParsing="false" onAction="#onUpdateComputerButtonClick" prefHeight="38.0" prefWidth="136.0" text="Update" />
            </children>
            <opaqueInsets>
               <Insets bottom="20.0" top="20.0" />
            </opaqueInsets>
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </VBox>
         <VBox alignment="CENTER" prefHeight="356.0" prefWidth="602.0" spacing="20.0">
            <children>
               <TextField fx:id="fxNewName" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="Name" />
               <TextField fx:id="fxNewType" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="Type" />
               <TextField fx:id="fxNewEnclosureName" layoutX="10.0" layoutY="152.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="Enclosure Name" />
               <TextField fx:id="fxNewCpuName" layoutX="10.0" layoutY="175.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="CPU Name" />
               <TextField fx:id="fxNewRamName" layoutX="10.0" layoutY="105.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="RAM Name" />
               <TextField fx:id="fxNewHardDiskCapacity" layoutX="10.0" layoutY="128.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="Hard Disk Capacity" />
               <TextField fx:id="fxNewGpuName" layoutX="10.0" layoutY="152.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="GPU Name" />
               <TextField fx:id="fxNewPowerSupply" layoutX="10.0" layoutY="36.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="Power Supply" />
               <TextField fx:id="fxNewPrice" layoutX="10.0" layoutY="198.0" minHeight="-Infinity" minWidth="-Infinity" prefHeight="26.0" prefWidth="514.0" promptText="Price" />
            </children>
         </VBox>
      </children>
   </HBox>
   <HBox alignment="CENTER_RIGHT" prefHeight="106.0" prefWidth="875.0">
      <padding>
         <Insets right="120.0" />
      </padding>
   </HBox>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</VBox>
